<!--pages/movies/starts/starts-template.wxml-->
<template name='stars-template'>
  <view class='stars-container'>
    <block wx:for='{{stars}}' wx:item='item' wx:key='idx'>
       <image src="{{item?'/images/icon/star.png':'/images/icon/none-star.png'}}"></image>
    </block>
    <text>{{score}}</text>
  </view>
</template>
